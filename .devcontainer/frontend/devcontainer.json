{
  "name": "Taxos Frontend Development Environment",
  "dockerComposeFile": [
    "../../docker-compose.yml",
    "docker-compose.override.yml"
  ],
  "features": {
    "ghcr.io/devcontainers/features/python:1": {
      "version": "3.12"
    },
    "ghcr.io/devcontainers/features/docker-outside-of-docker:1": {}
  },
  "containerEnv": {
    "PYTHONPATH": "/workspaces/taxos"
  },
  "service": "frontend",
  "shutdownAction": "none",
  "workspaceFolder": "/workspaces/taxos",
  "mounts": [
    "source=${localEnv:USERPROFILE}/.config/git,target=/home/node/.config/git,type=bind,consistency=cached",
    "source=${localEnv:USERPROFILE}/.ssh,target=/home/node/.ssh,type=bind,consistency=cached"
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "GitHub.copilot",
        "GitHub.copilot-chat",
        "DrBlury.protobuf-vsc",
        "bradlc.vscode-tailwindcss",
        "esbenp.prettier-vscode"
      ]
    }
  },
  "postCreateCommand": "bash .devcontainer/frontend/on_create.sh",
  "postStartCommand": "bash .devcontainer/frontend/on_start.sh",
  "postAttachCommand": "bash .devcontainer/frontend/on_attach.sh"
}