{
	"name": "Taxos",
	"dockerComposeFile": [
		"../docker-compose.yml"
	],
	"service": "dev",
	"remoteUser": "vscode",
	"workspaceFolder": "/workspaces/taxos",
	"mounts": [
		"source=${localEnv:USERPROFILE}/.config/git,target=/home/vscode/.config/git,type=bind,consistency=cached",
		"source=${localEnv:USERPROFILE}/.ssh,target=/home/vscode/.ssh,type=bind,consistency=cached"
	],
	"containerEnv": {
		"PYTHONPATH": "/workspaces/taxos/backend:/workspaces/taxos/dev"
	},
	"postCreateCommand": "bash .devcontainer/on_create.sh",
	"postStartCommand": "bash .devcontainer/on_start.sh",
	"postAttachCommand": "bash .devcontainer/on_attach.sh",
	"customizations": {
		"vscode": {
			"extensions": [
				"GitHub.copilot",
				"GitHub.copilot-chat",
				"DrBlury.protobuf-vsc",
				"charliermarsh.ruff",
				"ms-python.python",
				"ms-python.vscode-pylance",
				"dbaeumer.vscode-eslint",
				"esbenp.prettier-vscode"
			]
		}
	},
	"features": {
		"ghcr.io/devcontainers/features/python:1": {
			"version": "3.12"
		},
		"ghcr.io/devcontainers/features/docker-outside-of-docker:1": {}
	},
	"shutdownAction": "none"
}